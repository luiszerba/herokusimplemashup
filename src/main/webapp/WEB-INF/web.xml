<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<servlet>
		<servlet-name>SolicitarSegundaVia</servlet-name>			
		<servlet-class>br.com.odontoprev.mashup.poc.sales.SolicitarSegundaVia</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LiberarSenha</servlet-name>		
		<servlet-class>br.com.odontoprev.mashup.poc.sales.LiberarSenha</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SolicitarSegundaVia</servlet-name>
		<url-pattern>/solicitarSegundaVia</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LiberarSenha</servlet-name>
		<url-pattern>/liberarSenha</url-pattern>
	</servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>